public class Problem_1 {
  public static void main(String[] args) {
    for (int i = 15; i < 75; i++) {
      if (i % 2 == 0 & !(i % 10 == 0)) {
        System.out.println("Num");
      } else if (i % 5 == 0 & !(i % 10 == 0)) {
        System.out.println("Ber");
      } else if (i % 10 == 0) {
        System.out.println("Number");
      } else {
        System.out.println(i);
      }
    }
  }
}
